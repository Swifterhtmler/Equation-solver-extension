{
    "manifest_version": 3,
    "name": "Rikematic",
    "version": "2.0",
    "description": "A math Extension",
    "action": {
        "default_popup": "index.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "permissions": ["activeTab"]
}
